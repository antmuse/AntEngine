{
    "LogPath": "Log/",
    "PidFile": "Log/pid.txt",
    "ShareMem": "GMAP/MainMem.map", //共享内存名
    "ShareMemSize": 1, //[1-10 * 1024], 1MB
    "AcceptPost": 10,  //[1-255]监听端口上侯命的请求数
    "ThreadPool": 3,   //[1-255]

    "Website": [
        {
            "Lisen": "0.0.0.0:80",
            "Type": 0, //0=http,1=https
            "Timeout": 30, //秒,0不超时
            "Path": "D:/Code/AntEngine/Bin/Web/"
        },
        {
            "Lisen": "0.0.0.0:443",
            "Type": 1,
            "Timeout": 30,
            "Path": "D:/Code/AntEngine/Bin/Web/"
        }
    ],

    "FileServer": [
        {
            "Port": 8080,
            "Type": 0,
            "Timeout": 30, //秒,0不超时
            "Path": "/home/antmuse/file"
        }
    ],

    "RtmpServer": {
        "Port": 8081,
        "Type": 0,
        "Timeout": 30, //秒,0不超时
        "Path": "/home/antmuse/file/rtmp"
    },

    "Proxy": [
        {
            "Type": 0, //0=tcp, 1=tls tcp, 2=http, 3=https
            "MaxSpeed": 10240, //每个连接，字节每秒
            "Timeout": 30, //秒,0不超时
            "Lisen": "0.0.0.0:9900",
            "Backend": "127.0.0.1:9901"
        }
    ]
}
